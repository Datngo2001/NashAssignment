<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CustomerSite</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CustomerSite.styles.css" asp-append-version="true" />
</head>

<body style="background-color: #F5F5FA; position: relative;">
    <partial name="Header/_HeaderPartial" />

    <div class="container-lg min-vh-100">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <partial name="Footer/_FooterPartial" />

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://kit.fontawesome.com/d36df8790b.js" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/fetchAPI.js" asp-append-version="true"></script>
    <script src="~/js/fetchController.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
